{
  "name": "ioc.cats.ng",
  "version": "1.0.0",
  "description": "Open Source Bootstrap Admin Template",
  "author": "Andre Shen",
  "homepage": "http://www.fancydemo.com",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng-swagger-gen && ng build",
    "deploy": "ng build --prod -vc false -oh none",
    "postdeploy": "cd /var/www/html && sudo rm -rf cats-bak && sudo mv cats cats-bak && sudo cp -r ~/CATS/source/iocCATS/www . && sudo mv www cats && cd ~/CATS/source/iocCATS/www-ng",
    "test": "ng-swagger-gen && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "enc": "cd src/environments && encrypt environment.ts environment.ts.enc",
    "dec": "cd src/environments && decrypt environment.ts.enc environment.ts"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta",
    "@agm/js-marker-clusterer": "^1.0.0-beta",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@angular/upgrade": "^6.0.0",
    "@swimlane/ngx-datatable": "^11.1.0",
    "angularfire2": "^5.0.0-rc",
    "bootstrap": "^4.0.0",
    "chart.js": "^2.7.0",
    "core-js": "^2.5.0",
    "firebase": "~4.12.1",
    "font-awesome": "^4.7.0",
    "js-marker-clusterer": "^1.0.0",
    "moment": "^2.20.0",
    "ng-spin-kit": "^5.1.0",
    "ng2-charts": "1.6.0",
    "ng2-date-picker": "^2.9.0",
    "ng2-select": "^2.0.0",
    "ngx-bootstrap": "^2.0.3",
    "ngx-papaparse": "^2.1.0",
    "ngx-select-ex": "^3.3.10",
    "rxjs": "6",
    "rxjs-compat": "^6.1.0",
    "simple-line-icons": "^2.4.1",
    "ts-helpers": "1.1.2",
    "zone.js": "0.8.20"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@compodoc/compodoc": "^1.1.0",
    "@types/jasmine": "2.8.4",
    "@types/node": "9.3.0",
    "codelyzer": "^4.0.0",
    "config-leaf": "^0.3.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.3",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-swagger-gen": "^0.11.3",
    "protractor": "5.2.2",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  }
}
